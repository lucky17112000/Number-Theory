//     freopen("output.txt", "w", stdout);
// #endif
// }
using namespace std;
const int N = 1e5 + 7;
vector<bool> sieve(N, true);
void makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah()
{
    ll n;
    cin >> n;
    map<ll, ll> factor;
    vector<ll> allPrime;
    for (int i = 2; i <= n; ++i)
    {
        if (sieve[i])
            allPrime.push_back(i);
    }
    ll cnt = 0;

    while (n > 1)
    {
        if (n % allPrime[cnt] == 0)
        {
            ++factor[allPrime[cnt]];
            n /= allPrime[cnt];
        }
        else
            ++cnt;
    }
    for (auto u : factor)
    {
        cout << u.first << " " << u.second << endl;
    }
}

int32_t main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    // hey_output();
    for (int i = 2; i * i <= N; ++i)
    {
        if (sieve[i])
        {
            for (int j = i + i; j <= N; j += i)
            {
                sieve[j] = false;
            }
        }
    }

    int t = 1;
    // cin >> t;
    while (t--)
    {
        makeMeHappyReducePainGoAhedNeverGiveUPIWilBeSuccessInshaAllah();
    }

    return 0;
}
